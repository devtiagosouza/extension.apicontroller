{
	"name": "apicontroller",
	"displayName": "apicontroller",
	"description": "gerador de classe controller para web api",
	"version": "0.0.1",
	"author": "TiagoSouza",
	"repository": "https://github.com/devtiagosouza/extension.apicontroller",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Languages",
        "Linters",
        "Snippets"
	],
	"activationEvents": [
		"onLanguage:csharp",
		"onCommand:extension.apicontroller"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.apicontroller",
				"title": "New Api Controller Class"
			}
		]
	},
	"menus": {
		"explorer/context": [
			{
				"group": "navigation@-1",
				"command": "extension.apicontroller"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^10.2.0",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}
